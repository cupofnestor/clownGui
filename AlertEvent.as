package {	import flash.events.Event;	public class AlertEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const ALERT:String = "alert";		public static const ACTION:String = "action";		public var alert:Object = new Object();		public function AlertEvent(type:String = AlertEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _alert:Object = null) 		{			alert = _alert;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new AlertEvent(type, bubbles, cancelable, alert);		}	}}