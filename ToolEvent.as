package {	import flash.events.Event;	public class ToolEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const TOOL_CHOSEN:String = "toolChosen";		public static const UNDO:String = "undo";		public static const DONE:String = "done";		public static const SNAP_PIC:String = "snap"				public var data:Object;				public function ToolEvent(type:String = ToolEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:* = null) 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new ToolEvent(type, bubbles, cancelable, data);		}	}}