package {	import flash.events.Event;	public class TrackerEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const IMAGE_SAVED:String = "imageSaved";		public static const FACE_RECT:String = "faceRect";				public var data:*;				public function TrackerEvent(type:String = TrackerEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:* = null) 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new TrackerEvent(type, bubbles, cancelable);		}	}}