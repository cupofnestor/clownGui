package {	import flash.events.Event;	public class ClownEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const DRAW_DONE:String = "drawDone";				public var data:String;				public function ClownEvent(type:String = ClownEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:String = "") 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new ClownEvent(type, bubbles, cancelable);		}	}}