package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;			public class palette extends MovieClip {		private var theme1:Object = {};		private var r,g,b,yel,bl,wh:uint;				public function palette() {			// constructor code			setupColors();			addEventListener(MouseEvent.MOUSE_UP, colorSelected);					}				private function setupColors():void		{			r	=	0xFF0000;			g	=	0x00ff00;			b	=	0x0000ff;			yel	=	0xffff00;			bl	=	0x000000;			wh	=	0xffffff;						theme1.color1	=	r;			theme1.color2	=	g;			theme1.color3	=	b;			theme1.color4	=	yel;			theme1.color5	=	bl;			theme1.color6	=	wh;						for (var k:String in theme1 ){				trace( k);				var tin:* = this.getChildByName(k) as colorTin;				tin.color = theme1[k];			}					}				private function colorSelected(m:MouseEvent):void{			if(m.target is colorTin){				var tin:colorTin = m.target as colorTin;				dispatchEvent(new PaletteEvent(PaletteEvent.COLOR_CHOSEN,false,false,tin.color));			}					}	}	}