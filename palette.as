package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.events.Event;	import flash.media.Sound;	import flash.media.SoundChannel;		public class palette extends MovieClip {		private var theme1:Object = {};		private var r,g,b,yel,bl,wh:uint;		private var currentColor:uint =  0xffffff;		private var currentTin:colorTin;				private var clk:Sound = new scratch();				private var sc:SoundChannel = new SoundChannel();				public function palette() {			// constructor code			setupColors();			addEventListener(MouseEvent.MOUSE_UP, colorSelected);			addEventListener(Event.ADDED_TO_STAGE, init);		}				public function get color():uint{			return currentColor;		}				private function init(e:Event):void{			dispatchEvent(new PaletteEvent(PaletteEvent.COLOR_CHOSEN,false,false,0xffffff));		}				private function setupColors():void		{			r	=	0xFF0000;			g	=	0x00ff00;			b	=	0x0000ff;			yel	=	0xffff00;			bl	=	0x000000;			wh	=	0xffffff;						theme1.color1	=	r;			theme1.color2	=	g;			theme1.color3	=	b;			theme1.color4	=	yel;			theme1.color5	=	bl;			theme1.color6	=	wh;						for (var k:String in theme1 ){				trace( k);				var tin:* = this.getChildByName(k) as colorTin;				tin.color = theme1[k];			}					}				private function colorSelected(m:MouseEvent):void{			if(m.target is colorTin){				currentTin = m.target as colorTin;				currentColor = currentTin.color;				dispatchEvent(new PaletteEvent(PaletteEvent.COLOR_CHOSEN,false,false,currentTin.color));				sc = clk.play();			}					}	}	}