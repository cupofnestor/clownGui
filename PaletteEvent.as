package {	import flash.events.Event;	public class PaletteEvent extends Event 	{		// Event types.				public static const EVENT_DEFAULT:String = "default";		public static const COLOR_CHOSEN:String = "colorChosen";				public var data:uint;				public function PaletteEvent(type:String = PaletteEvent.EVENT_DEFAULT, bubbles:Boolean = false, cancelable:Boolean = false, _data:uint = 0x000000) 		{			data = _data;			super(type, bubbles, cancelable);		} 		override public function clone():Event {			// Return a new instance of this event with the same parameters.			return new PaletteEvent(type, bubbles, cancelable);		}	}}