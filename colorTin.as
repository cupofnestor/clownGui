package  {		import flash.display.MovieClip;	import flash.display.Bitmap;			public class colorTin extends MovieClip {		private var fillColor:uint;		private var	TinMask:Bitmap;				public function colorTin() {			// constructor code			TinMask = new Bitmap(new tinMask);			TinMask.cacheAsBitmap = true;			TinColor.cacheAsBitmap = true;						this.mouseChildren = false;		}				public function set color(c:uint):void{			fillColor = c;			fill(fillColor);		}				public function get color():uint{			return fillColor;		}				public function fill(c:uint):void		{			TinColor.graphics.clear();			TinColor.graphics.beginFill(c);			TinColor.graphics.drawRect(0,0,this.width*2, this.height*2);			TinColor.addChild(TinMask);			TinColor.mask = TinMask;					}	}	}